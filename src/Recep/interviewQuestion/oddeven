package Questions;

public class OddEven {
    
//    (Numbers) Odd & Even numbers
//    Write  a method which can identifies given number is even or odd
//
//    EX:
//
//    identify(5) ->  "Odd"
//
//    identify(6) ->  "Even"
    

    public  static  String  identifyOddEven( int  n ) {

        return  n%2==0 ? "Even" : "odd" ;

    }
}
